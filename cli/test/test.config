[database]
dialect=postgres
url=dbname=db3 user=postgres
preserved_objects=preserved_objects
baseline.revision=1

[executedScriptsTable]
autoCreateDbMaintainScriptsTable=false
executedScriptsTableName=dbmaintain_scripts
fileNameColumnName=file_name
fileNameColumnSize=150
fileLastModifiedAtColumnName=file_last_modified_at
checksumColumnName=checksum
checksumColumnSize=50
executedAtColumnName=executed_at
executedAtColumnSize=20
succeededColumnName=succeeded

[scripts]
location=core/test/scripts/py
extensions.sql=database
extensions.ddl=database
extensions.py =python

[python-runner]
# use /usr/local/bin/python script_path to run
executable=`which python`
#executable= #use /path/to/script to run

[preserved_objects]
#schemas=
#tables=
#views=
#materializedViews=
#synonyms=
#sequences=
#data_in_schemas=
#data_in_tables=
